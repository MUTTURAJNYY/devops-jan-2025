pipeline {
    agent {
        label 'dev'
    }
    stages {
        stage('build') {
            steps {
                sh 'echo "my name is raj"'
            }
        }
        stage('test') {
            steps {
                sh 'echo "this is test"'
            }
        }
        stage('develope') {
            steps {
                sh 'echo "my muddu"'
            }
        }
    }
}